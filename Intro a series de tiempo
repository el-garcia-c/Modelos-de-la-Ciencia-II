#tarea
  
data <- read.csv("https://raw.githubusercontent.com/VictorGuevaraP/MachineLearning/master/publicidad.csv",header = TRUE,sep = ";")
print(data)
head(data,10)
tail(data,10)
mean(data)
summary(data)
plot(data$Ventas,data$Television,type="p",xlab = "Ventas",ylab = "Television")
